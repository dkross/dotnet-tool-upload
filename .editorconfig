# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
tab_width = 2
trim_trailing_whitespace = true

[*.{sln,cs}]
indent_size = 4
tab_width = 4

[*.sln]
end_of_line = crlf
indent_style = tab
